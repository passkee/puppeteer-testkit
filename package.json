{
    "name": "puppeteer-testkit",
    "version": "0.4.5-alpha.6",
    "description": "A toolkit for DOM & BOM operation with puppeteer in nodejs environment, which greatly improves test case development efficiency",
    "main": "src/TestKit.js",
    "scripts": {
        "test": "mocha -r register.js test/index.ts --watch --watch-extensions ts,json --recursive --full-trace --timeout 10000",
        "test-dev": "mocha -r register.js test/index.ts --watch --watch-extensions ts,json --recursive --inspect --full-trace --bail --debug --async-only --timeout 10000",
        "report": "mocha -r register.js test/index.ts  --recursive --full-trace -O reportDir=report --reporter JSON --timeout 10000"
    },
    "author": "stevenjc",
    "homepage": "https://github.com/passkee/puppeteer-testkit",
    "bugs": "https://github.com/passkee/puppeteer-testkit/issues",
    "keywords": [
        "puppeteer",
        "dom",
        "bom",
        "toolkit",
        "test"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/passkee/puppeteer-testkit.git"
    },
    "license": "MIT",
    "typings": "typings/index.d.ts",
    "peerDependencies": {
        "puppeteer": "^1.14.0"
    },
    "files": [
        "src",
        "typings",
        "browser",
        "index.js"
    ],
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.11.4",
        "@types/puppeteer": "^1.12.3",
        "chai": "^4.2.0",
        "colors": "^1.3.3",
        "fs-extra": "^7.0.1",
        "mocha": "^5.2.0",
        "qs": "^6.7.0",
        "ts-mocha": "^6.0.0",
        "ts-node": "^8.0.2",
        "tsconfig-paths": "^3.8.0",
        "tslib": "^1.9.3",
        "typescript": "^3.0.0"
    },
    "dependencies": {
        "blink-diff": "^1.0.13",
        "qs": "^6.7.0"
    }
}
